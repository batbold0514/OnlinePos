<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib prefix="s" uri="/struts-tags"%>
<thead>
	<tr>
		<th>id</th>
		<th>name</th>
	</tr>
</thead>
<s:iterator value="articleList">
	<tbody>
		<tr>
			<td><s:property value="id" /></td>
			<td><s:property value="name" /></td>
		</tr>
	</tbody>
</s:iterator>