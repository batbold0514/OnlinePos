$(document).ready(function()
{
	$(".questions .panel-heading").click(function()
	{
		var href = $(this).children().children().last().attr("href");
		$(href).collapse('toggle');
		$(this).children().children().first().toggleClass("fa-caret-right");
		$(this).children().children().first().toggleClass("fa-caret-down");
	});
});