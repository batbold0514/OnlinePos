<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib prefix="s" uri="/struts-tags"%>
<div class="hidden" id="page">
	<s:url action="category-list" includeParams="none" />
</div>
<div class="panel panel-info" id="mainPanel">
	<div class="panel-heading">
		<h3 class="panel-title text-center">
			<span class="icon icon-category"></span>&nbsp;
			<s:text name="Category" />
		</h3>
	</div>
	<div class="panel-body">
		<s:form cssClass="form-horizontal">
			<div class="row">
				<div class="col-sm-12">
					<s:hidden key="id" />
					<div class="form-group">
						<label for="name" class="control-label col-sm-4 col-lg-offset-1 col-lg-3">
							<s:text name="name" />(*):
						</label>
						<div class="col-sm-7 col-lg-6">
							<s:textfield key="name" cssClass="form-control input-sm" id="name" disabled="true" />
						</div>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-sm-offset-1 col-sm-5 col-lg-offset-4 col-lg-3">
					<div class="form-group">
						<div class="col-sm-12">
							<b>
								<s:text name="requiredAttributes" />(*):
							</b>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="article_name" value="true" id="article_name" disabled="true" />&nbsp;
									<s:text name="article_name" />
								</label>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox" >
								<label>
									<s:checkbox key="barCode" value="true" disabled="true" />&nbsp;
									<s:text name="barCode" />
								</label>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="count" value="true" id="count" disabled="true" />&nbsp;
									<s:text name="count" />
								</label>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="measuring_unit" value="true" id="measuring_unit" disabled="true" />&nbsp;
									<s:text name="measuring_unit" />
								</label>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="minCount" value="true" id="minCount" disabled="true" />&nbsp;
									<s:text name="minCount" />
								</label>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="location" value="true" id="location" disabled="true" />&nbsp;
									<s:text name="location" />
								</label>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="description" value="true" id="description" disabled="true" />&nbsp;
									<s:text name="description" />
								</label>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="owner" value="true" id="owner" disabled="true" />&nbsp;
									<s:text name="owner" />
								</label>
							</div>
						</div>
					</div>
				</div>
				<div class="col-sm-5 col-lg-3">
					<div class="form-group">
						<div class="col-sm-12">
							<b>
								<s:text name="additionAttributes" />:
							</b>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="buyPrice" id="buyPrice" disabled="true" />&nbsp;
									<s:text name="buyPrice" />
								</label>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="sellPrice" id="sellPrice" disabled="true" />&nbsp;
									<s:text name="sellPrice" />
								</label>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="packageWeight" id="packageWeight" disabled="true" />&nbsp;
									<s:text name="packageWeight" />
								</label>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="moisture" id="moisture" disabled="true" />&nbsp;
									<s:text name="moisture" />
								</label>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="article_size" id="article_size" disabled="true" />&nbsp;
									<s:text name="article_size" />
								</label>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="colour" id="colour" disabled="true" />&nbsp;
									<s:text name="colour" />
								</label>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="partNumber" id="partNumber" disabled="true" />&nbsp;
									<s:text name="partNumber" />
								</label>
							</div>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-12">
							<div class="checkbox">
								<label>
									<s:checkbox key="serial" id="serial" disabled="true" />&nbsp;
									<s:text name="serial" />
								</label>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-sm-offset-1 col-sm-3 col-lg-offset-4 col-lg-2">
					<a onclick="window.history.back()" class="btn btn-back btn-block">
						<span class="fa fa-arrow-circle-o-left"></span>&nbsp;
						<s:text name="back" />
					</a>
				</div>
			</div>
		</s:form>
	</div>
</div>